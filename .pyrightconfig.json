{
  "venvPath": ".",
  "venv": ".venv",
  "pythonVersion": "3.13",
  "include": ["src"],
  "extraPaths": ["src"],
  "exclude": ["**/__pycache__", "**/node_modules", "**/migrations"],
  "reportMissingImports": "none",
  "reportMissingTypeStubs": false,
  "typeCheckingMode": "basic",
  "executionEnvironments": [
    {
      "root": ".",
      "pythonVersion": "3.13",
      "extraPaths": ["src"],
      "python": ".venv/bin/python"
    }
  ]
}
